<%= form_for @listing, :remote => true, :html => { :multipart => true } do |f| %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :available %>
  <%= f.check_box :available %>
  <%= f.file_field :photo %>
  <%= f.submit %>
<% end %>
